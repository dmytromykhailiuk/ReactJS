{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "baseUrl": "./src",
    "paths": {
      "@shared/*": ["./shared/*"],
      "@mainpage/*": ["./pages/main*"],
      "@models/*": ["./models/*"],
      "@mocks/*": ["./mocks/*"],
    }
  }
}
